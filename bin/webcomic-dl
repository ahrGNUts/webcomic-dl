#!/usr/bin/python
import webcomic_dl
from webcomic_dl.comics import *
from webcomic_dl import cli, Comic
import requests
print(webcomic_dl)
def getComic(url):
	for c in Comic.__subclasses__():
		print("trying " + c.__name__)
		if c.match(args.url):
			print("using " + c.__name__)
			return c(args.url)

args=cli.getArgs()
print(getComic(args.url).downloadImg())
